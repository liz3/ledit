{
  "mode_name": "SQL",
  "key_words": [
    "SELECT",
    "IF",
    "AND",
    "OR",
    "EXISTS",
    "LIKE",
    "ANY",
    "WHERE",
    "RETURN",
    "JOIN",
    "NULL",
    "JOIN",
    "COUNT",
    "PRIMARY",
    "IN",
    "INSERT",
    "OUTER",
    "INNER",
    "TABLE",
    "VIEW",
    "ORDER",
    "BY",
    "UPDATE",
    "DROP",
    "CREATE",
    "THEN",
    "ELSE",
    "INTO",
    "VALUES",
    "ROWNUM",
    "TRUNCATE",
    "CHECK",
    "USING",
    "CONSTRAINT",
    "WITH",
    "DEFAULT",
    "AS"
  ],
  "keywords_case_sensitive": false,
  "special_words": ["now", "text", "serial", "bigserial", "varchar", "gen_random_uuid"],
  "single_line_comment": "--",
  "multi_line_comment": ["/*", "*/"],
  "escape_character": "\\",
  "file_extensions": ["sql"],
  "string_characters": "\"'`"
}
